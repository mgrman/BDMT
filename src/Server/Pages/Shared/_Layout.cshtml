<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" />
    <title>BDMT</title>
    <base href="~/" />
    <link href="_content/BDMT.Client/css/bootstrap/bootstrap.min.css" rel="stylesheet" />
    <link href="_content/BDMT.Client/css/app.css" rel="stylesheet" />
    <link rel="apple-touch-icon" sizes="512x512" href="_content/BDMT.Client/icon-512.png" />
    <link rel="icon" type="image/png" href="_content/BDMT.Client/favicon.ico">
    @RenderSection("Header", false)
</head>

<body>
    @RenderBody()
    <div id="blazor-error-ui">
        <environment include="Staging,Production">
            An error has occurred. This application may no longer respond until reloaded.
        </environment>
        <environment include="Development">
            An unhandled exception has occurred. See browser dev tools for details.
        </environment>
        <a href="" class="reload">Reload</a>
        <a class="dismiss">🗙</a>
    </div>
    @RenderSection("Scripts", false)
</body>
</html>